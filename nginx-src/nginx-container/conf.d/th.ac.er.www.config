    server {
       listen 80;
       server_name er.co.th www.er.co.th;

       location /.well-known/acme-challenge {
           root /etc/letsencrypt/webrootauth;
           default_type "text/plain";
       }

       location / {
           #return 301 https://$server_name$request_uri;
           return 301 https://www.er.co.th$request_uri;
       }
    }

    server {
       listen 443 ssl http2;
       server_name er.co.th www.er.co.th;

       #ssl_certificate /etc/letsencrypt/live/er.co.th/fullchain.pem;
       #ssl_certificate_key /etc/letsencrypt/live/er.co.th/privkey.pem;
       include /var/nginx/conf.d/ssl/snippets/ssl-th.ac.er.www.conf;

       # ssl_dhparam /etc/nginx/ssl/dhparam.pem;
       include /var/nginx/conf.d/ssl/snippets/ssl-params.conf;

       location / {
           root   /var/nginx/html/th.ac.er.www;
           index  index.html index.htm;
       }

       error_page   500 502 503 504  /50x.html;
       location = /50x.html {
           root   /var/nginx/nginx/html/th.ac.er.www;
       }
    }
